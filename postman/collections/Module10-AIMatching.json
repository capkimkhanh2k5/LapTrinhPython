{
    "info": {
        "_postman_id": "module10-ai-matching",
        "name": "Module 10: AI Matching",
        "description": "API AI Matching - 8 endpoints",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "auth": {
        "type": "bearer",
        "bearer": [
            {
                "key": "token",
                "value": "{{token}}",
                "type": "string"
            }
        ]
    },
    "item": [
        {
            "name": "10.1 Job Matching",
            "item": [
                {
                    "name": "Get Matching Candidates for Job",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "type": "text/javascript",
                                "exec": [
                                    "pm.test('Status 200', () => pm.response.to.have.status(200));",
                                    "pm.test('Response is array', () => {",
                                    "    var json = pm.response.json();",
                                    "    pm.expect(json.results || json).to.be.an('array');",
                                    "});"
                                ]
                            }
                        }
                    ],
                    "request": {
                        "method": "GET",
                        "url": {
                            "raw": "{{base_url}}/jobs/:id/matching-candidates?limit=10",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "jobs",
                                ":id",
                                "matching-candidates"
                            ],
                            "query": [
                                {
                                    "key": "limit",
                                    "value": "10"
                                },
                                {
                                    "key": "min_score",
                                    "value": "0.5",
                                    "disabled": true
                                }
                            ],
                            "variable": [
                                {
                                    "key": "id",
                                    "value": "{{job_id}}"
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Get Matching Jobs for Recruiter",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "type": "text/javascript",
                                "exec": [
                                    "pm.test('Status 200', () => pm.response.to.have.status(200));"
                                ]
                            }
                        }
                    ],
                    "request": {
                        "method": "GET",
                        "url": {
                            "raw": "{{base_url}}/recruiters/:id/matching-jobs?limit=10",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "recruiters",
                                ":id",
                                "matching-jobs"
                            ],
                            "query": [
                                {
                                    "key": "limit",
                                    "value": "10"
                                },
                                {
                                    "key": "min_score",
                                    "value": "0.5",
                                    "disabled": true
                                }
                            ],
                            "variable": [
                                {
                                    "key": "id",
                                    "value": "{{recruiter_id}}"
                                }
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "10.2 AI Matching Core",
            "item": [
                {
                    "name": "Calculate Match Score",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "type": "text/javascript",
                                "exec": [
                                    "pm.test('Status 200 or 201', () => {",
                                    "    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
                                    "});",
                                    "pm.test('Has score field', () => {",
                                    "    var json = pm.response.json();",
                                    "    pm.expect(json).to.have.property('score');",
                                    "});"
                                ]
                            }
                        }
                    ],
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"job_id\": {{job_id}},\n  \"recruiter_id\": {{recruiter_id}}\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{base_url}}/ai-matching/calculate",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "ai-matching",
                                "calculate"
                            ]
                        }
                    }
                },
                {
                    "name": "Get Match Detail",
                    "request": {
                        "method": "GET",
                        "url": {
                            "raw": "{{base_url}}/ai-matching/:jobId/:recruiterId",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "ai-matching",
                                ":jobId",
                                ":recruiterId"
                            ],
                            "variable": [
                                {
                                    "key": "jobId",
                                    "value": "{{job_id}}"
                                },
                                {
                                    "key": "recruiterId",
                                    "value": "{{recruiter_id}}"
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Batch Calculate Scores",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"job_ids\": [1, 2, 3],\n  \"recruiter_ids\": [1, 2]\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{base_url}}/ai-matching/batch-calculate",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "ai-matching",
                                "batch-calculate"
                            ]
                        }
                    }
                },
                {
                    "name": "Get Top Matches",
                    "request": {
                        "method": "GET",
                        "url": {
                            "raw": "{{base_url}}/ai-matching/top-matches?limit=20",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "ai-matching",
                                "top-matches"
                            ],
                            "query": [
                                {
                                    "key": "limit",
                                    "value": "20"
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Refresh Match Scores",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"job_id\": {{job_id}}\n}",
                            "options": {
                                "raw": {
                                    "language": "json"
                                }
                            }
                        },
                        "url": {
                            "raw": "{{base_url}}/ai-matching/refresh",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "ai-matching",
                                "refresh"
                            ]
                        }
                    }
                },
                {
                    "name": "Get Matching Insights",
                    "request": {
                        "method": "GET",
                        "url": {
                            "raw": "{{base_url}}/ai-matching/insights",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "ai-matching",
                                "insights"
                            ]
                        }
                    }
                }
            ]
        }
    ]
}